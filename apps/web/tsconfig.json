{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": ["."],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["vite/client"],
    "strict": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
